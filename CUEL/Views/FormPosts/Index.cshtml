@model IEnumerable<CUEL.Models.FormPost>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_STLayout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.AppUser.UserName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DiscussionForm.Title)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Title)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.File)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FileType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Added)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.AppUser.UserName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DiscussionForm.Title)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Title)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Description)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.File)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FileType)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Added)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.FormPostID }) |
            @Html.ActionLink("Details", "Details", new { id=item.FormPostID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.FormPostID })
        </td>
    </tr>
}

</table>
